<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@project) do |f| %>
        <%= f.label :title %>
        <%= f.text_field :title %>

        <%= f.label :description %>
        <%= f.email_field :description %>

        <%= f.submit "Create Project", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
